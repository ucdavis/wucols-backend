{
  "name": "wucols-functions",
  "version": "1.0.0",
  "description": "Azure Function to export JSON data from WUCOLS to Azure Blob Storage",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ucdavis/wucols-backend.git"
  },
